{
  "name": "kalambo",
  "version": "0.2.4",
  "description": "Kalambo data system",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/client/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prettier": "prettier --single-quote --trailing-comma all --write \"{src,test}/**/*.{ts,tsx}\"",
    "clean": "rimraf lib/*",
    "build": "yarn run clean && tsc && babel lib/core -d lib/core --presets env && babel lib/client -d lib/client --presets env",
    "test": "jest",
    "prepare": "yarn run test && yarn run build"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"]
  },
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "@types/uuid": "^3.4.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "fetch-mock": "^5.12.1",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.10",
    "typescript": "^2.4.2",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "@types/flat": "^0.0.28",
    "@types/graphql": "^0.10.2",
    "@types/mongodb": "^2.2.9",
    "@types/pegjs": "^0.10.0",
    "@types/sequelize": "^4.0.68",
    "flat": "^4.0.0",
    "graphql": "^0.10.5",
    "lodash": "^4.17.4",
    "pegjs": "^0.10.0",
    "sequelize": "^4.4.3"
  }
}
